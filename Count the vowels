#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
     char str[100];
  fgets(str, sizeof(str), stdin);

  int count = 0;
  for (int i = 0; str[i] != '\0'; i++) {
    char ch = tolower(str[i]); 
    if (ch == 'a' || ch == 'e' || ch == 'i' || ch == 'o' || ch == 'u') {
      count++;
    }
  }

  printf("%d\n", count);
    return 0;
}
