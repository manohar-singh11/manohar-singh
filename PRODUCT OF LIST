#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int n;
    scanf("%d", &n);

    double product = 1.0;
    for (int i = 0; i < n; i++) {
        int value;
        scanf("%d", &value);
        product *= value;
    }

    printf("The Product of list value is %.2lf\n", product);


    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}
