#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
    int year;
  scanf("%d", &year);

  if (year % 100 == 0) {
    if (year % 400 == 0) {
      printf("The Given Year %d is a Leap Year.", year);
    } else {
      printf("The Given Year %d is Not a Leap Year.", year);
    }
  } else {
    if (year % 4 == 0) {
      printf("The Given Year %d is a Leap Year.", year);
    } else {
      printf("The Given Year %d is Not a Leap Year.", year);
    }
  }

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    
    return 0;
}
