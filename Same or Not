#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
     int n;
    scanf("%d", &n);
    int arr[n];
    int even_count = 0;
    int odd_count = 0;
    for (int i = 0; i < n; i++) {
        scanf("%d", &arr[i]);
        if (arr[i] % 2 == 0) {
            even_count++;
        } else {
            odd_count++;
        }
    }
    if (even_count == n) {
        printf("Even");
    } else if (odd_count == n) {
        printf("Odd");
    } else {
        printf("Mixed");
    }
    
    
    
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    return 0;
}
