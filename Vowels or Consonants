#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char ch;
    scanf(" %c", &ch);

    if (isalpha(ch)) {
        ch = toupper(ch);
        if (ch == 'A' || ch == 'E' || ch == 'I' || ch == 'O' || ch == 'u') {
            printf("The Character %c is Vowel\n", ch);
        } else {
            printf("The Character %c is Consonant\n", ch);
        }
    } else {
        printf("Invalid Input\n");
    }

    return 0;
}
