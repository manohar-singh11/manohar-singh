#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */

    int num;
    scanf("%d", &num);
    if (num % 2 == 0) {
        printf("The given input %d is Even", num);
    } else {
        printf("The given input %d is Odd", num);
    }
     
    return 0;
}
