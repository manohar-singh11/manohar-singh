#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    char name[50];
    
    fgets(name,sizeof(name),stdin);
    name[strcspn(name,"\n")]=0;
    printf("Hai %s!\tWelcome to Programming Language...\n",name);
    return 0;
}
