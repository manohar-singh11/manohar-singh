#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
     int num;
    scanf("%d", &num);

    if (num > 0) {
        printf("The Given Number %d is Positive.\n", num);
    } else if (num < 0) {
        printf("The Given Number %d is Negative.\n", num);
    } else {
        printf("The Given Number %d is Zero.\n", num);
    }

    
    return 0;
}
